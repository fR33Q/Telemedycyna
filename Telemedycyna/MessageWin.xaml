<Window x:Class="Telemedycyna.MessageWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Telemedycyna"
        mc:Ignorable="d"
        Title="Błąd" Height="300" Width="300" VerticalAlignment="Center" HorizontalAlignment="Center" ResizeMode="NoResize" WindowStartupLocation="CenterScreen" >
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="80*"/>
            <RowDefinition Height="50*"/>
        </Grid.RowDefinitions>
        <Label Name="MessageLabel" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Segoe UI Light" FontSize="22" Foreground="Black" Grid.Row="0"/>
        <Button Name="AcceptBTN" Content="Rozumiem" Width="250" Height="50" FontFamily="Segoe UI Light" FontSize="22" Grid.Row="1" Click="AcceptBTN_Click"/>
    </Grid>
</Window>
